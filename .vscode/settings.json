{
    "files.associations": {
        "*.rsh": "javascript"
    },
    "cSpell.enableFiletypes": [
        "!css",
        "!javascript",
        "!javascriptreact",
        "!json",
        "!scss",
        "!yaml",
        "!yml",
        "markdown"
    ],
    "cSpell.language": "en,en-GB,en-US",
    "javascript.suggest.completeFunctionCalls": true,
    "cSpell.words": [
        "Ipfs",
        "Terracell",
        "Terraland",
        "Terrasset",
        "TRAS",
        "TRCL",
        "TRLD"
    ],
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.formatOnSave": true
}
